<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.backend.mapper.UserMapper">
    
    <select id="findByUsername" resultType="com.app.backend.entity.User">
        SELECT * FROM pd_user WHERE username = #{username}
    </select>
    
</mapper>